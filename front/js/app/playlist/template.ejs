<% for ( var i = 0; i < songs.length; i++ ) {%>
    <% var song = songs[i]; %>
    <% if ( song.artist ) {%>
        <li class="playlist__song" data-song-url="<%= song.url %>">
            <span class="playlist__artist"><%= song.artist %></span>
            <span class="playlist__title"><%= song.artist %></span>
        </li>
    <% } %>
<% } %>